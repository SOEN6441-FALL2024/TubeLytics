GET     /               controllers.HomeController.index(query: String ?= null)
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /search                     controllers.YouTubeController.search(query: String)
POST    /search                     controllers.YouTubeController.search(query: String)
GET     /videos/tag/:tag            controllers.YouTubeController.getVideosByTag(tag: String)
GET     /wordstats/:query           controllers.YouTubeController.wordStats(query: String)
